def derive_wallets(mnemonic, currency, numderive):
    """Use the subprocess library to call the php file script from Python"""
    command = f'php ./hd-wallet-derive/hd-wallet-derive.php -g --mnemonic="{mnemonic}" --numderive="{numderive}" --currency="{currency}" --format=json' 
    
    p = subprocess.Popen(command, stdout=subprocess.PIPE, shell=True)
    (output, err) = p.communicate()
   
    keys = json.loads(output)
    return  keys

    currencies = {"eth", "btc-test", "btc"}
    numderive = 3   

    keys = {}
for currency in currencies:
    keys[currency]= derive_wallets(os.getenv('mnemonic'), currency, numderive=3)

    eth_privkey = keys['eth'][0]['privkey']
btc_privkey = keys['btc-test'][0]['privkey']

print(json.dumps(eth_privkey, indent=4, sort_keys=True))
print(json.dumps(btc_privkey, indent=4, sort_keys=True))